generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model ACTORS_CREATORS {
  ANC_ID            String              @id(map: "PK_ACTORS_CREATORS") @db.Char(10)
  ANC_NAME          String?             @db.NVarChar(30)
  ANC_DESC          String?
  ANC_AVATAR        Bytes?              @db.Image
  R_ID              String?             @db.Char(10)
  ROLES             ROLES?              @relation(fields: [R_ID], references: [R_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_ROLES_FILMS")
  FILM_COLABORATORS FILM_COLABORATORS[] @ignore
}

model COUNTRY {
  C_ID   String  @id(map: "PK_COUNTRY") @db.Char(10)
  C_NAME String  @db.NVarChar(30)
  FILMS  FILMS[]
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model FILM_COLABORATORS {
  FC_CHARACTERNAME String?          @db.NVarChar(30)
  F_ID             String?          @db.Char(10)
  R_ID             String?          @db.Char(10)
  ANC_ID           String?          @db.Char(10)
  ACTORS_CREATORS  ACTORS_CREATORS? @relation(fields: [ANC_ID], references: [ANC_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_ACTORS_CREATORS_FILMS")
  FILMS            FILMS?           @relation(fields: [F_ID], references: [F_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_FILMS_FILMS")
  ROLES            ROLES?           @relation(fields: [R_ID], references: [R_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_ROLES_FILM_COLABORATORS_FILMS")

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model FILM_GENRES {
  F_ID   String @db.Char(10)
  G_ID   String @db.Char(10)
  GENRES GENRES @relation(fields: [G_ID], references: [G_ID], onUpdate: NoAction, map: "FK_FENRES_FILM_GENRES")
  FILMS  FILMS  @relation(fields: [F_ID], references: [F_ID], onUpdate: NoAction, map: "FK_FILMS_FILM_GENRES")

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model FILM_KEYWORDS {
  F_ID     String?   @db.Char(10)
  KW_ID    String?   @db.Char(10)
  FILMS    FILMS?    @relation(fields: [F_ID], references: [F_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_FILMS_FILM_KEYWORDS_FILMS")
  KEYWORDS KEYWORDS? @relation(fields: [KW_ID], references: [KW_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_KEYWORDS_FILM_KEYWORDS_FILMS")

  @@ignore
}

model FILM_SEASON_SOURCE {
  SS_ID     String  @id(map: "PK_FILM_SEASON_SOURCE") @db.Char(10)
  SS_NAME   String? @db.NVarChar(10)
  SS_SOURCE String  @db.VarChar(200)
  F_ID      String? @db.Char(10)
  FILMS     FILMS?  @relation(fields: [F_ID], references: [F_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_FILM_SEASON_SOURCE_FILMS")
}

model FILMS {
  F_ID               String               @id(map: "PK_FILMS") @db.Char(10)
  F_NAME             String               @db.NVarChar(50)
  F_DESC             String               @db.NVarChar(200)
  F_RELEASEYEAR      String               @db.Char(4)
  F_AVGRATING        Decimal              @db.Decimal(4, 2)
  F_LIMITEDAGE       Int?                 @db.TinyInt
  F_THUMBNAIL        Bytes                @db.Image
  S_ID               String               @db.Char(10)
  C_ID               String               @db.Char(10)
  COUNTRY            COUNTRY              @relation(fields: [C_ID], references: [C_ID], onUpdate: NoAction, map: "FK_COUNTRY_FILMS")
  STATUSES           STATUSES             @relation(fields: [S_ID], references: [S_ID], onUpdate: NoAction, map: "FK_STATUSES_FILMS")
  FILM_COLABORATORS  FILM_COLABORATORS[]  @ignore
  FILM_GENRES        FILM_GENRES[]        @ignore
  FILM_KEYWORDS      FILM_KEYWORDS[]      @ignore
  FILM_SEASON_SOURCE FILM_SEASON_SOURCE[]
  RATINGS            RATINGS[]            @ignore
  TRAILER            TRAILER[]
  USERS_LIST_FILMS   USERS_LIST_FILMS[]   @ignore
}

model GENRES {
  G_ID        String        @id(map: "PK_GENRES") @db.Char(10)
  G_NAME      String        @db.NVarChar(30)
  FILM_GENRES FILM_GENRES[] @ignore
}

model KEYWORDS {
  KW_ID         String          @id(map: "PK_KEYWORDS") @db.Char(10)
  KW_TITLE      String?         @db.VarChar(25)
  FILM_KEYWORDS FILM_KEYWORDS[] @ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model RATINGS {
  RA_STARTNUMS Int?    @db.TinyInt
  RA_TOTALRATE Float?
  F_ID         String? @db.Char(10)
  U_ID         String? @db.Char(10)
  FILMS        FILMS?  @relation(fields: [F_ID], references: [F_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_FILMS_RATINGS_FILMS")
  USERS        USERS?  @relation(fields: [U_ID], references: [U_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_USERS_FILMS")

  @@ignore
}

model ROLES {
  R_ID              String              @id(map: "PK_ROLES") @db.Char(10)
  R_NAME            String?             @db.NVarChar(30)
  ACTORS_CREATORS   ACTORS_CREATORS[]
  FILM_COLABORATORS FILM_COLABORATORS[] @ignore
  USERS             USERS[]
}

model STATUSES {
  S_ID   String  @id(map: "PK_STATUSES") @db.Char(10)
  S_NAME String  @db.NVarChar(20)
  FILMS  FILMS[]
}

model TRAILER {
  TR_ID     String @id(map: "PK_TRAILER") @db.Char(10)
  TR_NAME   String @db.NVarChar(100)
  TR_SOURCE String @db.VarChar(100)
  F_ID      String @db.Char(10)
  FILMS     FILMS  @relation(fields: [F_ID], references: [F_ID], onUpdate: NoAction, map: "FK_TRAILER_FILMS")
}

model USERS {
  U_ID             String             @id(map: "PK_USERSPRIMARY") @db.Char(10)
  U_NAME           String?            @db.NVarChar(50)
  U_PHONE          String?            @db.VarChar(11)
  U_EMAIL          String?            @db.VarChar(25)
  U_AVATAR         Bytes?             @db.Image
  R_ID             String?            @db.Char(10)
  ROLES            ROLES?             @relation(fields: [R_ID], references: [R_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_ROLES_USERS_FILMS")
  RATINGS          RATINGS[]          @ignore
  USERS_LIST_FILMS USERS_LIST_FILMS[] @ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model USERS_LIST_FILMS {
  F_ID  String? @db.Char(10)
  U_ID  String? @db.Char(10)
  FILMS FILMS?  @relation(fields: [F_ID], references: [F_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_FILMS_USERS_LIST_FILMS_FILMS")
  USERS USERS?  @relation(fields: [U_ID], references: [U_ID], onDelete: NoAction, onUpdate: NoAction, map: "FK_USERS_USERS_LIST_FILMS_FILMS")

  @@ignore
}
